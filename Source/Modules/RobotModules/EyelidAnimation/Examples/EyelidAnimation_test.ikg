<?xml version="1.0"?>

<group title="EyelidAnimation Example">
	
		<module
			class = "Constant"
			name = "MyConstant"
			outputsize  = "2"
			data  = "1 2"
		/>
		<module
		  class       =   "FunctionGenerator"
		  name        =   "SinGen"
		  type        =   "sine"
		  frequency  =  "0.001"
		  offset      =   "0.0"
		  amplitude   =   "1"
		  shift       =   "0.0"
		/>

		<module
			class = "EyelidAnimation"
			name = "EyelidAnimationInstance"
			parameter1  = "1.23"
			parameter2  = "12"
		/>
		
        <connection source = "MyConstant.OUTPUT" target = "EyelidAnimationInstance.INPUT1" />
        <connection source = "SinGen.OUTPUT"  target = "EyelidAnimationInstance.INPUT2" />

		<view name="View">
			<plot 
				source="SinGen.OUTPUT" 
				
				x="0" y="0" width="100" height="100"
				min="0"
				max="2"
			/>
			<plot 
				source="EyelidAnimationInstance.OUTPUT" 
				x="0" y="100" width="100" height="100"
				min="0"
				max="2"
			/>
		</view>

</group>
 
