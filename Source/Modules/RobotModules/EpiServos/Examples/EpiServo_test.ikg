<?xml version="1.0"?>

<group title="MinimalModule Example">
	
		<module
			class = "Constant"
			name = "Goal_position"
			_data = "
			180 180 180 180 180 180
			180 180 180 180 180 180
			180 180 180 180 180 180
			180"
			data = "
			180 180 180 180 180 180
			"
		/>
		
		<module
			class = "Constant"
			name = "Goal_current"
			data = "0 0 0 0 0 0"

		/>

		<module
			class = "Constant"
			name = "Torque_enable"
			data = "0 0 0 0 0 0"

		/>

		<module
			class = "EpiServos"
			name = "EpiServos"
			log_level = "8"
			robot = "EpiBlue"
			simulate = "false"
		/>

        <_connection  source = "Torque_enable.OUTPUT"  target = "EpiServos.TORQUE_ENABLE" />
        <connection  source = "Goal_position.OUTPUT"  target = "EpiServos.GOAL_POSITION" />
        <_connection  source = "Goal_current.OUTPUT"  target = "EpiServos.GOAL_CURRENT" />

<view name="Test servos" >
	<plot  buffer_size = "50" style = "" show_title = "true" frame-style = "" max = "180" min = "-180" direction = "vertical" select = "" source = "EpiServos.PRESENT_POSITION" show_frame = "false" height = "381" x = "40" title = "Present position" color = "" width = "401" y = "40"/>
	<plot  buffer_size = "50" style = "" show_title = "true" frame-style = "" max = "2000" min = "-2000" direction = "vertical" select = "" source = "EpiServos.PRESENT_CURRENT" show_frame = "false" height = "361" x = "40" title = "Present current" color = "" width = "401" y = "420"/>
	<slider-horizontal  frame-style = "" show_title = "true" max = "360" show_values = "true" step = "0.01" min = "0" style = "" labels = "" select = "0" count = "6" parameter = "Goal_position.data" x = "440" height = "381" show_frame = "false" title = "Goal position" width = "361" y = "40"/>
	<slider-horizontal  frame-style = "" show_title = "true" max = "360" show_values = "true" step = "0.01" min = "0" style = "" labels = "" select = "0" count = "6" parameter = "Goal_current.data" x = "440" height = "361" show_frame = "false" title = "Goal current" width = "361" y = "420"/>
	<slider-horizontal  frame-style = "" show_title = "true" max = "1" show_values = "true" step = "1" min = "0" style = "" labels = "" select = "0" count = "6" parameter = "Torque_enable.data" x = "800" height = "381" show_frame = "false" title = "Torque enable" width = "121" y = "40"/>
</view>













    <module class = "Logger" name = "Logger" />
</group>
 
