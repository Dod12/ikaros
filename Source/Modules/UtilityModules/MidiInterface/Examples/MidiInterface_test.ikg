<?xml version="1.0"?>

<group title="MidiInterface Example">
	
		<module
			class = "Constant"
			name = "MyConstant"
			outputsize  = "2"
			data  = "1 2"
		/>
		<module
		  class       =   "FunctionGenerator"
		  name        =   "SquareGen"
		  type        =   "square"
		  frequency  =  "0.001"
		  offset      =   "0.0"
		  amplitude   =   "1"
		  shift       =   "0.0"
		/>

		<module
			class = "MidiInterface"
			name = "MidiInterfaceInstance"
			port="1"
		/>
		
        <_connection source = "MyConstant.OUTPUT" target = "MidiInterfaceInstance.INPUT1" />
        <_connection source = "SquareGen.OUTPUT"  target = "MidiInterfaceInstance.INPUT2" />

<view name="View" >
	<plot  frame-style = "" style = "" show_title = "true" buffer_size = "50" min = "0" show_frame = "false" height = "100" color = "" title = "Plot" width = "100" direction = "vertical" select = "" source = "SquareGen.OUTPUT" y = "0" x = "0" max = "2"/>
	<plot  frame-style = "" style = "" show_title = "true" buffer_size = "50" min = "0" show_frame = "false" height = "100" color = "" title = "Plot" width = "100" direction = "vertical" select = "" source = "MidiInterfaceInstance.OUTPUT" y = "100" x = "0" max = "2"/>
	<table  frame-style = "" style = "" show_title = "true" colorize = "true" decimals = "1" scrollable = "false" direction = "normal" y = "20" label_y = "" source = "MidiInterfaceInstance.OUTPUT" title = "Midi" label_x = "" width = "141" show_frame = "false" height = "61" x = "160"/>
</view>



</group>
 
