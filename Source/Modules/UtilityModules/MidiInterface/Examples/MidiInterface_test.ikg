<?xml version="1.0"?>

<group title="MidiInterface Example">
	
		<module
			class = "Constant"
			name = "MyConstant"
			outputsize  = "2"
			data  = "1 2"
		/>
		<module
		  class       =   "FunctionGenerator"
		  name        =   "SquareGen"
		  type        =   "square"
		  frequency  =  "0.001"
		  offset      =   "0.0"
		  amplitude   =   "1"
		  shift       =   "0.0"
		/>

		<module
			class = "MidiInterface"
			name = "MidiInterfaceInstance"
			port="1"
		/>
		
        <_connection source = "MyConstant.OUTPUT" target = "MidiInterfaceInstance.INPUT1" />
        <_connection source = "SquareGen.OUTPUT"  target = "MidiInterfaceInstance.INPUT2" />

<view name="View" >
	<plot  max = "2" x = "0" y = "0" source = "SquareGen.OUTPUT" select = "" direction = "vertical" height = "100" show_frame = "false" width = "100" title = "Plot" color = "" frame-style = "" show_title = "true" style = "" min = "0" buffer_size = "50"/>
	<plot  max = "2" x = "0" y = "100" source = "MidiInterfaceInstance.OUTPUT" select = "" direction = "vertical" height = "100" show_frame = "false" width = "100" title = "Plot" color = "" frame-style = "" show_title = "true" style = "" min = "0" buffer_size = "50"/>
	<table  x = "160" height = "61" show_frame = "false" width = "141" label_x = "" direction = "normal" scrollable = "false" source = "MidiInterfaceInstance.OUTPUT" y = "20" label_y = "" decimals = "1" colorize = "true" title = "Midi" frame-style = "" show_title = "true" style = ""/>
	<switch  x = "160" y = "60" height = "41" width = "161" title = "Switch Title" label = "Debug" parameter = "MidiInterfaceInstance.debug" single_trig = "true" value = "1" xindex = "0" yindex = "0" show_title = "false" show_frame = "false" style = "" frame-style = ""/>
</view>




</group>
 
