<?xml version="1.0"?>

<group title="Expression Example">
	
		<module
			class = "Constant"
			name = "MyConstant"
			outputsize  = "2"
			data  = "0.5 0.7"
		/>
		<module
		  class       =   "FunctionGenerator"
		  name        =   "SquareGen"
		  type        =   "square"
		  frequency  =  "0.01"
		  offset      =   "0.0"
		  amplitude   =   "1"
		  shift       =   "0.0"
		/>
        <module
		  class       =   "FunctionGenerator"
		  name        =   "SinGen"
		  type        =   "Sin"
		  frequency  =  "0.01"
		  offset      =   "0.0"
		  amplitude   =   "1"
		  shift       =   "0.0"
		/>

		<module
			class = "Expression"
			name = "ExpressionInstance"
			expression  = "x+y"
			inputs  = "x,y"
			debug="false"
		/>
		
        <connection sourcemodule = "MyConstant" source = "OUTPUT" targetmodule = "ExpressionInstance" target = "x" />
        <connection sourcemodule = "SquareGen" source = "OUTPUT" targetmodule = "ExpressionInstance" target = "y" />
        <connection sourcemodule = "SinGen" source = "OUTPUT" targetmodule = "ExpressionInstance" target = "y" />

		


 
<view name="View">

	<plot
		max = "2"
		h = "400"
		w = "200"
		y = "10"
		x = "10"
		source = "OUTPUT"
		min = "0"
		module = "SquareGen"
		class = "plot"
		buffer_size = "50"
		direction = "vertical"
		width = "341"
		height = "341"
	/>

	<plot
		max = "2"
		h = "400"
		w = "200"
		y = "400"
		x = "10"
		source = "OUTPUT"
		min = "0"
		module = "ExpressionInstance"
		class = "plot"
		buffer_size = "50"
		direction = "vertical"
		width = "301"
		height = "321"
	/>

</view>
</group>