<?xml version="1.0"?>

<group title="Stop Example">
	
		
		<module
		  class       =   "FunctionGenerator"
		  name        =   "singen"
		  type        =   "sin"
		  frequency  =  "0.1"
		  offset      =   "0.0"
		  amplitude   =   "1"
		  shift       =   "0.0"
		/>

		<module
			class = "Stop"
			name = "StopInstance"
			termination_criterion  = "0.5"
			wait  = "150"
			select="0"
			debug="no"
		/>
		
      
        <connection sourcemodule = "singen" source = "OUTPUT" targetmodule = "StopInstance" target = "INPUT" />

		<view name="View">
			<object class="Plot " 
				module="singen" 
				source="OUTPUT" 
				x="0" y="0" w="1" h="1"
				min="-1"
				max="1"
			/>
			
		</view>

</group>
 
