<?xml version="1.0"?>

<group title="MidiFilter Example">
	
		<module
			class = "MidiInterface"
			name = "Midi"
			inport  = "1"
		/>
		
		
		<module
			class = "MidiFilter"
			name = "MidiFilterInstance"
			description="filter selects val 0 and 1 for byte
				0, selects values from 0-10 in a size 10
				array, and outputs byte 3 on those arrays"
			filter="153 48 58;185 10 20"
			debug="yes"

			
		/>
		
        <connection source = "Midi.OUTPUT" target = "MidiFilterInstance.INPUT" />
        
<view name="Midifilter test view" >
	<table  x = "20" height = "81" show_frame = "false" width = "201" label_x = "" direction = "normal" scrollable = "false" source = "Midi.OUTPUT" y = "20" label_y = "" decimals = "1" colorize = "true" title = "Midi message" frame-style = "" show_title = "true" style = ""/>
	<table  x = "20" height = "61" show_frame = "false" width = "401" label_x = "" direction = "normal" scrollable = "false" source = "MidiFilterInstance.OUTPUT_1" y = "420" label_y = "" decimals = "1" colorize = "true" title = "Midifilter out 1" frame-style = "" show_title = "true" style = ""/>
	<table  x = "20" height = "101" show_frame = "false" width = "401" label_x = "" direction = "normal" scrollable = "false" source = "MidiFilterInstance.OUTPUT_2" y = "640" label_y = "" decimals = "1" colorize = "true" title = "Midifilter out 2" frame-style = "" show_title = "true" style = ""/>
	<bar-graph  frame-style = "" show_title = "true" max = "10" min = "0" lineCap = "" fill = "" style = "" labels = "" direction = "vertical" source = "MidiFilterInstance.OUTPUT_1" lineJoin = "" width = "401" lineWidth = "1" color = "" title = "Midifilter 1" show_frame = "false" height = "201" y = "260" x = "20"/>
	<bar-graph  frame-style = "" show_title = "true" max = "10" min = "0" lineCap = "" fill = "" style = "" labels = "" direction = "vertical" source = "MidiFilterInstance.OUTPUT_2" lineJoin = "" width = "401" lineWidth = "1" color = "" title = "Midifilter 2" show_frame = "false" height = "201" y = "480" x = "20"/>
</view>





</group>
 
