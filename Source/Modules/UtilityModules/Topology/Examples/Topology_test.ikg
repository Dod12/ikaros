<?xml version="1.0"?>

<group title="Topology Example">
	
		<module
			class = "Constant"
			name = "MyConstant"
			outputsize  = "2"
			data  = "1 2 3"
		/>
		<module
		  class       =   "FunctionGenerator"
		  name        =   "SquareGen"
		  type        =   "square"
		  frequency  =  "0.1"
		  offset      =   "0.0"
		  amplitude   =   "1"
		  shift       =   "0.0"
		/>
		<_connection source = "SquareGen.OUTPUT"  target = "Ensemble.EXCITATION" />

		<_module
		class="NucleusEnsemble"
		name="Ensemble"
		size="3"
		/>

		<module
			class = "Topology"
			name = "TopologyInstance"
			type="nearest-neighbor_2D"
			tensor_size_x="3"
			tensor_size_y="3"
			
			debug="yes"
			
		/>
		
        <_connection source = "MyConstant.OUTPUT" target = "TopologyInstance.SIZE_X" />
		<_connection source = "MyConstant.OUTPUT" target = "TopologyInstance.SIZE_Y" />
        

<view name="View" >
	<table  x = "220" height = "241" show_frame = "false" width = "421" label_x = "" direction = "normal" scrollable = "false" source = "TopologyInstance.OUTPUT" y = "160" label_y = "" decimals = "0" colorize = "true" title = "Topology" frame-style = "" show_title = "true" style = ""/>
	<image  x = "0" y = "160" height = "200" width = "200" title = "Topology" source = "TopologyInstance.OUTPUT" file = "" index = "" module = "" command = "" format = "gray" scale = "both" opacity = "1" scales = "no" min_x = "0" max_x = "1" min_y = "0" max_y = "1" flipXAxis = "no" flipYAxis = "no" flipXCanvas = "no" flipYCanvas = "no" show_title = "true" show_frame = "false" style = "" frame-style = ""/>
</view>




</group>
 
