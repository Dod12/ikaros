<?xml version="1.0"?>

<group title="Topology Example">
	

		<_module
			class = "Topology"
			name = "TopologyInstance"
			_type=""
			tensor_size_x="3"
			tensor_size_y="3"
			
			debug="yes"
			
		/>
		<module
			class = "Topology"
			name = "OneToOne"
			type="one-to-one"
			tensor_size_x="3"
			tensor_size_y="3"
			
			debug="yes"
			
		/>
		<module
			class = "Topology"
			name = "NN2D"
			type="nearest-neighbor_2D"
			tensor_size_x="3"
			tensor_size_y="3"
			
			debug="yes"
			
		/>

		<module
			class = "Topology"
			name = "Circle"
			type="circle"
			tensor_size_x="3"
			tensor_size_y="1"
			
			debug="yes"
			
		/>
		<module
			class = "Topology"
			name = "Random"
			type="random"
			tensor_size_x="3"
			tensor_size_y="3"
			random_limit="0.9"
			allow_self_connection="no"
			
			debug="yes"
			
		/>
		


