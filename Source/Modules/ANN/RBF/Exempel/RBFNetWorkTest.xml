<?xml version="1.0"?>
<network>
    <modules>
	
		<module 
			class = "InputFile" 
			name = "TRAINING_SET" 
			filename = "training_set.txt">
				<iterations>10000</iterations>
		</module>
	
		<module
			class = "RBFNetwork" 
			name = "RBFNETWORK">
			inputsize = "8"
			outputsize = "3"
			training_set_size = "12"
			mean_error_limit	=	"0.01"
		</module>
    </modules>
	
    <connections>
		<connection 
			sourcemodule = "TRAINING_SET"    
			source = "A" 
			targetmodule = "RBFNETWORK" 
			target = "INPUT"
		/>
		<connection 
			sourcemodule = "TRAINING_SET"    
			source = "B" 
			targetmodule = "RBFNETWORK" 
			target = "INPUT_TARGET"
		/>
    </connections>

</network>
