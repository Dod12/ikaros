<?xml version="1.0"?>

<group title="LidarSensor Example">
	
	<module
		class = "LidarSensor"
		name = "LidarSensor"
		serial_port = "/dev/tty.usbserial-0001"
		baud_rate = "256000"
	/>

	<view  name = "LidarSensor">
		<grid width="800" height="800" show_title = "true" flipYAxis="yes" flipYCanvas="yes" title= "Gridmap" direction = "horizontal" source = "LidarSensor.GRID" x = "10" y = "10" valueLow="0" valueHigh="1"/>
	</view>

    <module class = "Logger" name = "Logger" />
</group>
 
