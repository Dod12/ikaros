<?xml version="1.0"?>

<group name="ODriveController" title="ODriveController Example">

		<module
			class = "ODriveController"
			name = "Platform"
			input_mode  = "3"
			control_mode  = "0"
		/>

		<view name="ODrive" >
			<button height = "25" width = "100" y = "45" x = "10" label="Left" title="Left" command="Platform.left" />
			<button height = "25" width = "100" y = "45" x = "230" label="Right" title="Right" command="Platform.right" />
			<button height = "25" width = "100" y = "10" x = "120" label="Forward" title="Forward" command="Platform.forward" />
			<button height = "25" width = "100" y = "45" x = "120" label="Back" title="Back" command="Platform.back" />
		</view>

    	<module class = "Logger" name = "Logger" />
</group>
 
