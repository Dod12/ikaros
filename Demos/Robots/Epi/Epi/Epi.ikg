<!-- External Motor control -->
<input name = "SERVO_ANGLES"      		target = "EXT_SERVO_ANGLE.INPUT"/>
<input name = "SERVO_ANGLES_AMPLITUDE"	target = "EXT_SERVO_ANGLE_A.INPUT"/>

<module class = "Gate" name = "EXT_SERVO_ANGLE"/>  <!-- Dummy module -->
