    />
    <module
        class = "Gate"
        name = "P"
    />
    <connection sourcemodule="P1" source="OUTPUT" targetmodule="P" target="INPUT" />
    <connection sourcemodule="P2" source="OUTPUT" targetmodule="P" target="INPUT" />
    
    <module
        class = "FunctionGenerator"
        name = "Enable"
