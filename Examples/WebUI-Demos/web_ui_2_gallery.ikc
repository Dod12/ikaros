        max = "0.9"
    />

    <module
        class = "FunctionGenerator"
        name = "G1"
        type = "sin"
        frequency = "0.1"
    />
    <module
        class = "FunctionGenerator"
        name = "G2"
        type = "sin"
        frequency = "0.1"
    />
    <module
        class = "Gate"
        name = "G"
    />
    <connection sourcemodule="G1" source="OUTPUT" targetmodule="G" target="INPUT" />
    <connection sourcemodule="G2" source="OUTPUT" targetmodule="G" target="INPUT" />

    <module
        class = "FunctionGenerator"
        name = "P1"
        type = "sin"
        frequency = "0.05"
