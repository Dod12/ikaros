            x="0" y="0" 
        />
		
		<object
			x="0" y="0" 
			class="BarGraph" 
			module="R5" 
			source="OUTPUT"
			title = "Bar Graph 1"
			fill = "#567"
			color = "black"
			frame="none"
		/>
		
		<object
			x="4" y="0" 
			class="BarGraph" 
			title="BarGraph" 
			module="R5" 
			source="OUTPUT"
		/>
		
		<object class="PolarPlot" module="PP25" source="OUTPUT"
