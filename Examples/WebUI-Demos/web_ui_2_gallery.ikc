        <object class="PolarPlot" title="Bull's Eye" module="PP25" source="OUTPUT"
			grid_color = "red"
			grid_fill = "white"
			grid_stroke_width = "5"
			color = "black"
			fill = "yellow"
			stroke_width = "9"
