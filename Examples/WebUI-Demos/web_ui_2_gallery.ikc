<?xml version="1.0"?>

<!-- Example of the different WebUI 2.0 objects filled with random values. -->

<group name="WebUI Demo">

    <view name="Epi the Robot">
        <epi-head label="Epi"  gazeModule="G" gazeSource="OUTPUT" pupilModule="P" pupilSource="OUTPUT" x="20" y="20" width="300" height="300"  visibleModule="Enable" visibleSource="OUTPUT" />
    </view>
    
    <view name="Controls">
        <button label="Press Me" module="R23T" parameter="data" x="40" y="40" width="100" height="100" />
        <button label="Me Too" module="R23T" parameter="data" x="40" y="140" width="100" height="100" />
        <button label="Sometimes" module="R23T" parameter="data" x="40" y="240" width="100" height="100" enableModule="Enable" enableSource="OUTPUT" />

        <switch label="Alarm" module="SwitchValue" parameter="data" x="40" y="360" width="100" height="100" />

        <drop-down-menu label="" list="X,Y,Z" module="Options" parameter="data" x="40" y="420" width="100" height="50" />
        <drop-down-menu label="" module="Options2" parameter="data" list_module="OptionsForMenu" list_parameter="OUTPUT" x="40" y="520" width="100" height="50" />

        <slider-horizontal label="X" module="CCC" parameter="data" count="3" x="200" y="40" width="100" height="100" />
        <slider-horizontal label="X" module="CCC" parameter="data" count="3" x="300" y="40" width="100" height="100" />

        <bar-graph
            x="440" y="40"
            width="301"
            height="301"
            module="CCC"
            source="OUTPUT"
            title = "Bar Graph X"
            labels = "A, B, C, D, E, F, G"
            style = ""
        />

        <plot
            x="440" y="440"
            width="301"
            height="301"
            module="CCC"
            source="OUTPUT"
            title = "Plot"
            _labels = "A, B, C, D, E, F, G"
            style = ""
        />
    </view>

