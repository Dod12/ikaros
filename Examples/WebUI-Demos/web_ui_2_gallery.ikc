<?xml version="1.0"?>

<!-- Example of the different WebUI 2.0 objects filled with random values. -->

<group name="WebUI Demo">

     <view name="Markers">
        <marker
            x="20" y="20" width="200" height="200"
            title="Default Path"
            module="R23"
            source="OUTPUT"
            markerType = "circle"
            size = "0.05"
            show_frame = "yes"
        />

        <marker
            x="240" y="20" width="200" height="200"
            title="Default Path"
            module="R23"
            source="OUTPUT"
            fill = "yellow"
            markerType = "cross"
            size = "0.1"
            show_frame = "yes"
        />

        <marker
            x="460" y="20" width="200" height="200"
            title="Default Path"
            module="R23"
            source="OUTPUT"
            fill = "yellow"
            markerType = "cross"
            lineWidth = "12"
            lineCap = "round"
            color = "red"
            size = "0.3"
            show_frame = "yes"
        />

        <marker
            x="20" y="240" width="200" height="200"
            title="Default Path"
            module="R43"
            source="OUTPUT"
            order = "row"
            markerType = "circle"
            size = "0.05"
            scales = "yes"
            color = "red, green, blue"
            fill = "red, green, blue"
        />

        <marker
            x="240" y="240" width="200" height="200"
            title="Default Path"
            module="R43"
            source="OUTPUT"
            order = "col"
            markerType = "circle"
            size = "0.05"
            scales = "yes"
            color = "red, green, blue"
            fill = "red, green, blue"
        />

        <marker
            x="460" y="240" width="200" height="200"
            title="Default Path"
            module="R23"
            source="OUTPUT"
            fill = "yellow"
            labelType = "numbered"
            markerType = "cross"
            lineWidth = "12"
            lineCap = "round"
            color = "red"
            size = "0.3"
            scales = "yes"
        />

        <marker
            x="20" y="460" width="200" height="200"
            title="Labels"
            module="R23"
            source="OUTPUT"
            color = "red"
            fill = "black"
            markerType = "none"
            labelType = "labels"
            labels="Alpha,Bravo,Charlie,Delta"
            show_frame = "yes"
        />

        <marker
            x="240" y="460" width="200" height="200"
            title="Labels"
            module="R23"
            source="OUTPUT"
            markerType = "none"
            color = "red"
            fill = "black"
            labelType = "alphabetical"
            show_frame = "yes"
        />

        <marker
            x="460" y="460" width="200" height="200"
            title="Labels"
            module="R23"
            source="OUTPUT"
            markerType = "cross"
            lineWidth = "3"
            color = "blue"
            fill = "black"
            size = "0.2"
            labelType = "xy_value"
            labelDecimals = "1"
            labelPrefix = "("
            labelPostfix = ")"
            labelFont = "12px sans-serif"
            show_frame = "yes"
            labelAlign = "left"
            labelBaseline = "top"
            labelOffsetX = "10"
            labelOffsetY = "10"
            style = "background-image:linear-gradient(yellow,red)"
        />
    </view>

